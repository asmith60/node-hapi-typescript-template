{
    "check-coverage": false,
    "per-file": true,
    "include": [
      "src/**/*.ts",
      "src/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "json"
    ],
    "cache": true,
    "all": true,
    "instrument": false,
    "sourceMap": false,
    "report-dir": "./artifacts/coverage/istanbul"
}
